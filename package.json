  "scripts": {
    "flow": "node_modules/.bin/flow",
    "test": "cross-env NODE_ENV=test jest",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "clean-docs": "rimraf docs/*",
    "prepare:index-html": "node ./scripts/prepareIndexHtml.js",
    "predev": "npm run clean-docs",
    "dev": "cross-env NODE_ENV=dev webpack --config webpack.dev.config.js",
    "postdev": "react-snap",
    "predev-static": "rimraf docs/* &&  node ./scripts/prepareIndexHtml.js",
    "dev-static": "cross-env NODE_ENV=dev webpack --config webpack.dev.config.js",
    "preprod": "npm run clean-docs",
    "prod": "cross-env NODE_ENV=production webpack -p --config webpack.production.config.js",
    "postprod": "react-snap",
    "preprod-static": "npm run clean-docs",
    "prod-static": "cross-env NODE_ENV=production webpack -p --config webpack.production.config.js",
    "analyze": "rimraf docs/assets && cross-env NODE_ENV=production webpack -p --config webpack.analyze.config.js",
    "prestart": "npm run clean-docs && npm run prepare:index-html",
    "startold": "cross-env NODE_ENV=dev webpack-dev-server --config ./webpack.hot.reload.config.js --mode development",
    "start": "cross-env NODE_ENV=dev webpack-dev-server --config ./webpack.hot.reload.config.js --mode development",
    "serve-dev": "npm run dev && nodemon ./src/server/server.js",
    "serve-prod": "npm run prod && node ./src/server/server.js",
    "serve-dev-static": "npm run dev-static && nodemon ./src/server/server.js",
    "serve-prod-static": "npm run prod-static && node ./src/server/server.js",
    "check-types": "tsc"
  },
